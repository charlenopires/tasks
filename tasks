#!/usr/bin/env escript
%% -*- erlang -*-

% -mode(compile).
% -compile(native).

%TODO: See utf-8 in Erlang, how to work?

main(_) ->
  % ScriptDir = filename:dirname(filename:absname(escript:script_name())),
  {ok, ScriptDir} = file:get_cwd(),
  {ok, Listing} = file:list_dir_all(ScriptDir),
  io:format("~s~n", [Listing]),
  erlang:halt(0).
